(( $# == 0 )) || {
  warn 'Too many arguments'
  return 1
}

local ctx tmp
zstyle -g tmp

for ctx ("$tmp[@]"); do
  if [[ "$ctx" == :zoppo:internal:plugin:* ]] && zstyle -t "$ctx" loaded; then
    print -- "${ctx##:zoppo:internal:plugin:}"
  fi
done

# vim: ft=zsh sts=2 ts=2 sw=2 et fdm=marker fmr={{{,}}}
